  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fcortesf/eggify"
  },
  "bugs": {
    "url": "https://github.com/fcortesf/eggify/issues"
  },
  "homepage": "https://github.com/fcortesf/eggify#readme",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "unpkg": "./dist/index.global.js",
  "jsdelivr": "./dist/index.global.js",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs,iife --global-name Eggify --dts --minify --sourcemap --out-dir dist",
    "dev": "tsup src/index.ts --format esm,cjs,iife --global-name Eggify --watch --out-dir dist",
    "prepare": "npm run build",
    "test": "vitest"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@types/jsdom": "^27.0.0",
    "happy-dom": "^20.0.8",
